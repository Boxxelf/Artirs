{
	"bsonType": "object",
	"required": ["user_id", "token"],
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"user_id": {
			"bsonType": "string",
			"description": "用户ID"
		},
		"token": {
			"bsonType": "string",
			"description": "登录token"
		},
		"create_date": {
			"bsonType": "timestamp",
			"description": "创建时间",
			"forceDefaultValue": {
				"$env": "now"
			}
		},
		"expire_date": {
			"bsonType": "timestamp",
			"description": "过期时间"
		}
	}
}
